a,b=map(int,input().split(" "))
l1=[[i.strip() for i in input().split(" ")]for j in range(a)]
l2=[[i.strip() for i in input().split(" ")]for j in range(a)]
for i in range(a):
    if(i%2==0):
        for j in range(b):
            print(l1[i][j],end=" ")
        print()
    else:
        for j in range(b):
            print(l2[i][j],end=" ")
        print()
